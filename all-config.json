{
    "log": {
        "disabled": false,
        "level": "info",
        "output": "/usr/local/sing-box/sing-box.log",
        "timestamp": true
    },
    "dns": {
        "servers": [
            {
                "tag": "google-tls",
                "address": "local",
                "address_strategy": "prefer_ipv4",
                "strategy": "ipv4_only",
                "detour": "direct"
            },
            {
                "tag": "google-udp",
                "address": "8.8.8.8",
                "address_strategy": "prefer_ipv4",
                "strategy": "prefer_ipv4",
                "detour": "direct"
            }
        ],
        "strategy": "prefer_ipv4",
        "disable_cache": false,
        "disable_expire": false
    },
    "inbounds": [
        {
            "type": "vmess",
            "tag": "vmess-in",
            "listen": "0.0.0.0",
            "listen_port": 80,
            "tcp_fast_open": false,
            "sniff": false,
            "sniff_override_destination": false,
            "domain_strategy": "prefer_ipv4",
            "proxy_protocol": false,
            "users": [
                {
                    "name": "masjeho",
                    "uuid": "masjeho96",
                    "alterId": 0
                }
            ],
            "tls": {
                "enabled": true,
                "server_name": "servername.com",
                "alpn": [
                    "http/1.1",
                    "h2"
                ],
                "certificate_path": "/root/fullchain.cer",
                "key_path": "/root/private.key"
            },
            "transport": {
                "type": "ws",
                "path": "/vmess"
            }
        },
        {
            "type": "trojan",
            "tag": "trojan-in",
            "listen": "0.0.0.0",
            "listen_port": 443,
            "domain_strategy": "ipv4_only",
            "users": [
                {
                    "name": "username",
                    "password": "password"
                }
            ],
            "tls": {
                "enabled": true,
                "server_name": "servername.com",
                "certificate_path": "/root/fullchain.cer",
                "key_path": "/root/private.key"
            },
            "transport": {
                "type": "ws",
                "path": "/trojan-ws"
            }
        }
    ],
    "outbounds": [
        {
            "type": "direct",
            "tag": "direct"
        },
        {
            "type": "block",
            "tag": "block"
        },
        {
            "type": "dns",
            "tag": "dns-out"
        }
    ],
    "route": {
        "rules": [
            {
                "protocol": "dns",
                "outbound": "dns-out"
            },
            {
                "inbound": [
                    "vmess-in"
                    "trojan-in"
                ],
                "domain_suffix": "googlesyndication.com",
                "outbound": "direct"
            },
            {
                "inbound": [
                    "vmess-in"
                    "trojan-in"
                ],
                "geosite": [
                    
                    "oisd-full",
                    "rule-ads",
                    "rule-malicious"
                ],
                
                "outbound": "block"
            }
            
        ],
        "geoip": {
            "path": "geoip.db",
            "download_url": "https://github.com/malikshi/sing-box-geo/releases/latest/download/geoip.db",
            "download_detour": "direct"
        },
        "geosite": {
            "path": "geosite.db",
            "download_url": "https://github.com/malikshi/sing-box-geo/releases/latest/download/geosite.db",
            "download_detour": "direct"
        },
        "final": "direct",
        "auto_detect_interface": true
    }
}
