{
  "log": {
      "disabled": false,
      "level": "info",
      "output": "/usr/local/sing-box/sing-box.log",
      "timestamp": true
  },
  "dns": {
    "servers": [
        {
            "tag": "google-tls",
            "address": "local",
            "address_strategy": "prefer_ipv4",
            "strategy": "ipv4_only",
            "detour": "direct"
        },
        {
            "tag": "google-udp",
            "address": "8.8.8.8",
            "address_strategy": "prefer_ipv4",
            "strategy": "prefer_ipv4",
            "detour": "direct"
        }
    ],
    "strategy": "prefer_ipv4",
    "disable_cache": false,
    "disable_expire": false
  },
  "inbounds": [
    {
      "type": "vmess",
      "tag": "vmess-in",
      "listen": "::",
      "listen_port": 10001,
      "users": [
        {
          "uuid": "masjeho96",
          "alterId": 0
        }
      ],  
      "transport": {
        "type": "ws",
        "path": "/vmess",
        "max_early_data": 2048,
        "early_data_header_name": "Sec-WebSocket-Protocol"
      }
    },
    {
      "type": "trojan",
      "tag": "trojan-in",
      "listen": "::",
      "listen_port": 10003,
      "users": [
        {
          "password": "masjeho96"
        }
      ],
      "transport": {
        "type": "ws",
        "path": "/trojan-ws"
      }
    },
    {
      "type": "vless",
      "listen": "::",
      "listen_port": 10002,
      "users": [
        {
          "uuid": "masjeho96"
        }
      ],
      "transport": {
        "type": "ws",
        "path": "/vless"
      }
    }
  ],
  "outbounds": [
    {
      "type": "direct",
      "tag": "direct"
    },
    {
      "type": "block",
      "tag": "block"
    },
    {
      "type": "dns",
      "tag": "dns-out"
    }
  ],
  "route": {
    "rules": [
      {
        "protocol": "dns",
        "outbound": "dns-out"
      },
      {
        "inbound": [
            "vmess-in",
            "trojan-in",
            "vless-in"
        ],
        "domain_suffix": "googlesyndication.com",
        "outbound": "direct"
      },
      {
        "inbound": [
            "vmess-in",
            "trojan-in",
            "vmess-in"
        ],
        "geosite": [
            
            "oisd-full",
            "rule-ads",
            "rule-malicious"
        ],
        
        "outbound": "block"
      }
    ],
    "geoip": {
        "path": "geoip.db",
        "download_url": "https://github.com/malikshi/sing-box-geo/releases/latest/download/geoip.db",
        "download_detour": "direct"
    },
    "geosite": {
        "path": "geosite.db",
        "download_url": "https://github.com/malikshi/sing-box-geo/releases/latest/download/geosite.db",
        "download_detour": "direct"
    },
    "final": "direct",
    "auto_detect_interface": true
  }
}
